{
  "name": "@incognia/api",
  "author": "Incognia (https://us.incognia.com)",
  "repository": "https://github.com/inloco/incognia-node",
  "version": "3.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/api.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "dts watch",
    "build": "dts build",
    "test": "dts test",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "jsxBracketSameLine": true,
    "arrowParens": "avoid",
    "trailingComma": "none"
  },
  "size-limit": [
    {
      "path": "dist/api.cjs.production.min.js",
      "limit": "20 KB"
    },
    {
      "path": "dist/api.esm.js",
      "limit": "20 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^6.0.4",
    "@tsconfig/recommended": "^1.0.1",
    "@types/qs": "^6.9.7",
    "dts-cli": "^0.19.4",
    "husky": "^7.0.4",
    "nock": "^13.1.4",
    "size-limit": "^6.0.4",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "qs": "^6.10.1"
  }
}
